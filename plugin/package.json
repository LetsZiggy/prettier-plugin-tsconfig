{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "prettier-plugin-tsconfig-src",
	"type": "module",
	"engines": {
		"node": ">=24.0.0"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"scripts": {
		"build": "pnpm run prebuild:rm && pnpm run prebuild:tsc && pnpm exec parcel build && pnpm run postbuild:mv",
		"fix:eslint": "pnpm exec eslint --config ./eslint.config.js --max-warnings 0 --no-warn-ignored --fix",
		"fix:prettier": "pnpm exec prettier --config ./prettier.config.js --write",
		"postbuild:mv": "mv -f ./dist ../",
		"prebuild:rm": "rm -rf ./dist && rm -rf ../dist",
		"prebuild:tsc": "tsc --noEmit",
		"preinstall": "npx only-allow pnpm"
	},
	"peerDependencies": {
		"prettier": "^3.5.3"
	},
	"dependencies": {
		"prettier-plugin-tsconfig": "link:..",
		"strip-json-comments": "^5.0.3"
	},
	"devDependencies": {
		"@babel/types": "^7.28.0",
		"@eslint/js": "^9.30.1",
		"@parcel/bundler-library": "^2.15.4",
		"@parcel/config-default": "^2.15.4",
		"@parcel/packager-ts": "2.16.3",
		"@parcel/transformer-typescript-types": "2.16.3",
		"@stylistic/eslint-plugin": "^5.1.0",
		"@stylistic/eslint-plugin-migrate": "^4.4.1",
		"@total-typescript/ts-reset": "^0.6.1",
		"@total-typescript/tsconfig": "^1.0.4",
		"@types/node": "^25.0.3",
		"eslint": "^9.30.1",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-n": "^17.21.0",
		"eslint-plugin-promise": "^7.2.1",
		"eslint-plugin-sort-class-members": "^1.21.0",
		"eslint-plugin-sort-destructure-keys": "^2.0.0",
		"eslint-plugin-unicorn": "^62.0.0",
		"globals": "^17.0.0",
		"parcel": "^2.15.4",
		"prettier": "^3.6.2",
		"prettier-plugin-pkg": "^0.21.1",
		"tslib": "^2.8.1",
		"type-fest": "^5.3.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1"
	},
	"@parcel/resolver-default": {
		"packageExports": true
	},
	"source": "./src/index.ts",
	"targets": {
		"default": {
			"sourceMap": false,
			"distDir": "./dist"
		},
		"main": {
			"sourceMap": false,
			"distDir": "./dist"
		},
		"module": {
			"sourceMap": false,
			"distDir": "./dist"
		}
	}
}
